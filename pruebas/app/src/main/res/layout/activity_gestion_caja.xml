                android:text="Chuleton" />

            <Button
                android:id="@+id/bt_carne2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Entrecot" />

            <Button
                android:id="@+id/bt_carne3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Solomillo" />

            <Button
                android:id="@+id/bt_carne4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Cachopo" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/bt_pescado1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Dorada" />

            <Button
                android:id="@+id/bt_pescado2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Salmon" />

            <Button
                android:id="@+id/bt_pescado3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lubina" />

            <Button
                android:id="@+id/bt_pescado4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lenguado" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/bt_postre1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="t.queso" />

            <Button
                android:id="@+id/bt_postre2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Flan" />

            <Button
                android:id="@+id/bt_postre3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="A.con leche" />

            <Button
                android:id="@+id/bt_postre4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fruta" />
        </TableRow>
    </TableLayout>

    <Button
        android:id="@+id/bt_efectivo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="20dp"
        android:text="cobrar efectivo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TableLayout
        android:id="@+id/tabla_bebida"
        android:layout_width="379dp"
        android:layout_height="209dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="185dp"
        app:layout_constraintBottom_toTopOf="@+id/bt_efectivo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tabla_comida">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/bt_combinado1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Com1" />

            <Button
                android:id="@+id/bt_combinado2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Com2" />

            <Button
                android:id="@+id/bt_combinado3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Com3" />

            <Button
                android:id="@+id/bt_combinado4"
                android:layout_width="wrap_content"
