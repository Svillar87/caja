                android:text="AlbariÃ±o" />

            <Button
                android:id="@+id/bt_vino2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Verdejo" />

            <Button
                android:id="@+id/bt_vino3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Rioja" />

            <Button
                android:id="@+id/bt_vino4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ribera" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/bt_refresco1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Coca cola" />

            <Button
                android:id="@+id/bt_refresco2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Coca zero" />

            <Button
                android:id="@+id/bt_refresco3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="F.naranja" />

            <Button
                android:id="@+id/bt_refresco4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="F.limon" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/bt_cafe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Cafe" />

            <Button
                android:id="@+id/bt_infusion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Infusion" />

            <Button
                android:id="@+id/bt_te"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tea" />

            <Button
                android:id="@+id/bt_licor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Licor" />
        </TableRow>
    </TableLayout>

    <Button
        android:id="@+id/bt_clear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="158dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="33dp"
        android:text="Borrar ticket"
        app:layout_constraintBottom_toTopOf="@+id/bt_efectivo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/bt_tarjeta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="20dp"
        android:text="Cobrar Tarjeta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bt_efectivo"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_cuenta"
        android:layout_width="383dp"
        android:layout_height="56dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/bt_clear"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tabla_bebida" />

</androidx.constraintlayout.widget.ConstraintLayout>